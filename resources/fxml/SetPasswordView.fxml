<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="248.0" prefWidth="275.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.simpletimetracker.controllers.SetNewPasswordViewController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="252.0" prefWidth="285.0" VBox.vgrow="ALWAYS">
         <children>
            <PasswordField fx:id="newPassword" layoutX="66.0" layoutY="53.0" />
            <PasswordField fx:id="repeatNewPassword" layoutX="66.0" layoutY="124.0" />
            <Text layoutX="66.0" layoutY="38.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Set new password" />
            <Text layoutX="66.0" layoutY="110.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Confirm new password" />
            <Button fx:id="saveButton" layoutX="187.0" layoutY="194.0" mnemonicParsing="false" onAction="#onSaveButton" text="save" />
            <Button fx:id="discardButton" layoutX="54.0" layoutY="194.0" mnemonicParsing="false" onAction="#onDiscardButton" text="Discard" />
         </children>
    </AnchorPane>
  </children>
</VBox>
